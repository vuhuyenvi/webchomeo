                    <script>
                        var cartItems = []; // Mảng để lưu trữ các mục trong giỏ hàng

                        function themvaogiohang(button) {
                            var product = button.parentNode.parentNode;
                            var image = product.querySelector('img').src;
                            var name = product.querySelector('h3').innerText;
                            var price = product.querySelector('.price-box span').innerText;

                            var cartItems = document.getElementById('cart-items');
                            var cartItemElements = cartItems.getElementsByClassName("cart-item");

                            // Kiểm tra xem sản phẩm đã tồn tại trong giỏ hàng chưa
                            for (var i = 0; i < cartItemElements.length; i++) {
                                var cartItem = cartItemElements[i];
                                var itemName = cartItem.querySelector(".cart-item-name").innerText;

                                if (itemName === name) {
                                    var quantityInput = cartItem.querySelector(".cart-item-quantity");
                                    var quantity = parseInt(quantityInput.value);
                                    quantity += 1;
                                    quantityInput.value = quantity;

                                    capNhatThongTinGioHang();

                                    return; // Kết thúc hàm nếu sản phẩm đã tồn tại trong giỏ hàng

                                }
                            }
                            var newItem = document.createElement('li');
                            newItem.classList.add('cart-item');
                            newItem.innerHTML = `
                                                                              <img class="img_SP" src="${image}" alt="product-image">
                                                                              <div class="cart-item-info">
                                                                                <span class="cart-item-name">${name}</span>
                                                                                <span class="cart-item-price">${price}</span>
                                                                                <input type="number" class="cart-item-quantity" value="1" min="1" onchange="capNhatThongTinGioHang()">
                                                                                <img class="delete-icon" src="https://bizweb.dktcdn.net/100/432/370/themes/894869/assets/icon-remove.png?1665454509075" alt="delete-icon" onclick="xoaSanPham(this)">
                                                                              </div>
                                                                            `;
                            cartItems.appendChild(newItem);

                            capNhatThongTinGioHang();
                            scrollToTop();
                            hienThiGioHang();
                        }
                        function scrollToTop() {
                            var currentPosition = document.documentElement.scrollTop || document.body.scrollTop;

                            if (currentPosition > 0) {
                                window.requestAnimationFrame(scrollToTop);
                                window.scrollTo(0, currentPosition - currentPosition / 30);
                            }
                        }

                        function xoaSanPham(deleteIcon) {
                            var item = deleteIcon.parentNode.parentNode;
                            item.remove();

                            capNhatThongTinGioHang();
                        }

                        // New function to show/hide the cart items and info containers
                        function hienThiGioHang() {
                            var cartItemsContainer = document.getElementById('cart-items-container');
                            var cartInfoContainer = document.getElementById('cart-info');

                            if (cartItemsContainer.style.display === 'none') {
                                cartItemsContainer.style.display = 'block';
                                cartInfoContainer.style.display = 'block';
                            } else {
                                cartItemsContainer.style.display = 'none';
                                cartInfoContainer.style.display = 'none';
                            }
                        }

                        // New function to update the cart info
                        function capNhatThongTinGioHang() {
                            var cartItems = document.querySelectorAll('.cart-item');
                            var cartTotal = cartItems.length;
                            var cartSubtotal = 0;
                            var cartItemsCount = 0;
                            cartItems.forEach(function (item) {
                                var price = item.querySelector('.cart-item-price').innerText;
                                var quantity = item.querySelector('.cart-item-quantity').value;
                                cartSubtotal += parseInt(price) * quantity;
                                cartItemsCount += parseInt(quantity);
                            });
                            var cartTotalElement = document.getElementById('cart-total');
                            var cartSubtotalElement = document.getElementById('cart-subtotal');
                            var cartItemsCountElement = document.querySelector('#cart-icon .cart-items-count');
                            cartTotalElement.innerText = cartItemsCount;
                            cartSubtotalElement.innerText = cartSubtotal + '.000₫';
                            cartItemsCountElement.innerText = cartItemsCount;
                        }


                        function dongThongTinGioHang() {
                            var cartItemsContainer = document.getElementById('cart-items-container');
                            var cartInfoContainer = document.getElementById('cart-info');

                            cartItemsContainer.style.display = 'none';
                            cartInfoContainer.style.display = 'none';
                        }
                    </script>
